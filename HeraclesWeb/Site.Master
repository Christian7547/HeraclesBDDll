        <section class="info_section layout_padding typographyClass">
            <div class="container">
                <div class="row">
                    <div class="col-md-3">
                        <div class="info-logo">
                            <h2>HERACLES</h2>
                            <p>
                                Ven y entrena con nosotros
                            </p>
                        </div>
                    </div>
                    <div class="col-md-3">
